
module traits

trait Movable {
    func move(x int)
}

struct Elevator {
    height int
}

impl Movable for Elevator {
    impl func move(self mut Movable&, x int) {
        height = x
    }
}

func moveSomethingMovable(thing Movable &, x int) {
    thing.move(x)
}

func main() {
    let elevator = Elevator{
       height = 0
    }

    moveSomethingMovable(elevator)

    elevator.x
}
