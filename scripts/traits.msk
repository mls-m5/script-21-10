
module traits

trait Movable {
    func move(x int)
}

struct Elevator {
    height int
}

impl Elevator {
    trait Movable

    impl func move(self Elevator&, x int) {
        height = x
    }
}

func moveSomethingMovable(thing Movable *, x int) {
    thing->move(x)
}

func main() {
    let elevator = Elevator{
       height = 0
    }

    moveSomethingMovable(elevator, 10)

    return elevator.height
}
