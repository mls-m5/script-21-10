
module structmethods

struct Vec2 {
    x float
    y float
}

impl Vec2 {
    func abs() -> float() {
       return math.sqrt(x * x + y * y)
    }

    operator+(Vec2 v) {
       return Vec2{v.x + x, v.y + y}
    }
}

func main() {
    let vec1 = Vec2{2, 3}
    let vec2 = Vec2{4, 5}

    let vec3 = vec1 + vec2
    let abs = vec3.abs()

    log.print("length {}", abs)
}
